{
	"env": {
		"browser": true,
		"node": true
	},

	"rules": {
		"no-comma-dangle": 1,
		"no-cond-assign": 1,
		"no-constant-condition": 1,
		"no-dupe-keys": 1,
		"no-empty": 1,
		"no-ex-assign": 1,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": 0,
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-inner-declarations": [1, "both"],
		"no-invalid-regexp": 1,
		"no-irregular-whitespace": 1,
		"no-negated-in-lhs": 1,
		"no-obj-calls": 1,
		"no-reserved-keys": 1,
		"no-sparse-arrays": 1,
		"no-unreachable": 1,
		"use-isnan": 1,
		"valid-jsdoc": [1, {
			"prefer": {
				"returns": "return"
			},
			"requireReturn": false
		}],
		"valid-typeof": 1,

		"consistent-return": 1,
		"curly": [2, "all"],
		"dot-notation": 0,
		"eqeqeq": 1,
		"no-caller": 1,
		"no-else-return": 1,
		"no-eval": 1,
		"no-fallthrough": 1,
		"no-floating-decimal": 1,
		"no-implied-eval": 1,
		"no-lone-blocks": 1,
		"no-loop-func": 1,
		"no-multi-spaces": 0,
		"no-native-reassign": 1,
		"no-new-func": 1,
		"no-new-wrappers": 1,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-redeclare": 1,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-self-compare": 1,
		"no-sequences": 1,
		"no-unused-expressions": 1,
		"no-void": 1,
		"no-with": 1,
		"radix": 1,
		"vars-on-top": 1,

		"global-strict": [1, "always"],
		"no-extra-strict": 1,
		"strict": 1,

		"no-delete-var": 1,
		"no-label-var": 1,
		"no-shadow-restricted-names": 1,
		"no-undef": 1,
		"no-undefined": 1,
		"no-unused-vars": [1, {"vars": "all", "args": "all"}],
		"no-use-before-define": 1,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 1,

		"brace-style": [1, "1tbs", {"allowSingleLine": true}],
		"camelcase": 1,
		"comma-spacing": [1, {"before": false, "after": true}],
		"comma-style": [1, "last"],
		"consistent-this": [1, "self"],
		"eol-last": 1,
		"func-names": 0,
		"key-spacing": [0, { "align": "colon" }],
		"new-cap": 1,
		"new-parens": 1,
		"no-array-constructor": 1,
		"no-lonely-if": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-multiple-empty-lines": [1, {"max": 2}],
		"no-nested-ternary": 1,
		"no-new-object": 1,
		"no-space-before-semi": 1,
		"no-spaced-func": 1,
		"no-trailing-spaces": 1,
		"one-var": 1,
		"quote-props": [1, "as-needed"],
		"quotes": [1, "single", "avoid-escape"],
		"semi": [1, "always"],
		"space-after-keywords": [1, "always"],
		"space-before-blocks": [1, "always"],
		"space-in-brackets": [1, "never"],
		"space-in-parens": [0, "always"],
		"space-infix-ops": 1,
		"space-return-throw-case": 1,
		"space-unary-ops": [1, {"words": true, "nonwords": false}],
		"spaced-line-comment": [0, "always"]
	}
}
